@model issBlueMetal.Models.RawMateriyalPurchase

@{
    ViewBag.Title = "Details";
}

<div class="breadcrumbs-inline pt-3 pb-1" id="breadcrumbs-wrapper">
    <div class="container">
        <div class="row">
            <div class="col s10 m6 l6 breadcrumbs-left">
                <h5 class="breadcrumbs-title mt-0 mb-0 display-inline hide-on-small-and-down"><span>Raw Material Purchase Details</span></h5>

                <ol class="breadcrumbs mb-0">
                    <li class="breadcrumb-item">
                        <a href="~/RawMateriyalPurchases/Index">Index</a>
                    </li>
                    <li class="breadcrumb-item">
                        <a href="#">Details</a>
                    </li>

                </ol>
            </div>

        </div>
    </div>
</div>
<div class="row">
    <div class="col s12">
        <div class="card card-tabs">
            <div class="card-content">
                <table>
                    <tr>
                    <th  style="width: 15%">
                        <label class="col-lg-2 font-weight-bold">Id</label></th>
                    <th>
                        @Html.DisplayFor(model => model.id)
                    </th>
                    </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Arrival Place</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.ArrivalPlace.name)</th>
                </tr>
                <tr>
                    <th>   <label class="col-lg-2 font-weight-bold">Departure Place</label></th>
                    <th>    @Html.DisplayFor(model => model.DeparturePlace.name)</th>
                </tr>
                <tr>
                    <th> <label class="col-lg-2 font-weight-bold">Material Name</label></th>
                    <th>@Html.DisplayFor(model => model.materialName.name)</th>
                </tr>
                <tr>
                    <th>  <label class="col-lg-2 font-weight-bold">Staff</label></th>
                    <th>  @Html.DisplayFor(model => model.Staff.name)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Supplier</label>
                    </th>
                    <th>   @Html.DisplayFor(model => model.Supplier.name)</th>
                </tr>
                <tr>
                    <th>  <label class="col-lg-2 font-weight-bold">Vehicle</label></th>
                    <th> @Html.DisplayFor(model => model.Vehicle.vehicleNo)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Date Time</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.dateTime)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Type</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.type)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Weight</label>
                    </th>
                    <th>   @Html.DisplayFor(model => model.Weight)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Total Unit</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.totalUnit)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Raw Material Dept</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.rawMaterialDept)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Salary</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.salary)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Total Amount</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.totalAmount)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">JCB Salary</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.JcbSalary)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Net Amount</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.netAmount)</th>
                </tr>
                <tr>
                    <th>   <label class="col-lg-2 font-weight-bold">No of Load</label></th>
                    <th>  @Html.DisplayFor(model => model.noofLoad)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Production Dept</label>
                    </th>
                    <th>  @Html.DisplayFor(model => model.productionDept)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Salary Per Load</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.salaryPerLoad)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Rate Per Unit</label>
                    </th>
                    <th>@Html.DisplayFor(model => model.ratePerUnit)</th>
                </tr>
                <tr>
                    <th>
                        <label class="col-lg-2 font-weight-bold">Driver Salary</label>
                    </th>
                    <th> @Html.DisplayFor(model => model.driverSalary)</th>
                </tr>
                <tr>
                    <th>  <label class="col-lg-2 font-weight-bold">Vehicle Rent</label></th>
                   
                    <th>  @Html.DisplayFor(model => model.vehicleRent)</th>
                </tr>

                </table>
            <div class="header-elements">
                <div class="list-icons" style="text-align:right">
                    @Html.ActionLink("Back to List", "Index", null, htmlAttributes: new { @class = "btn btn-primary" })
                    @Html.ActionLink("Edit", "Edit", new { id = Model.id }, new { @class = "btn btn-success" })
                </div>
            </div>
        </div>
        </div>
    </div>
</div>
