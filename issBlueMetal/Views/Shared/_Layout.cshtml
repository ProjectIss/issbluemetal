<script>
    $(document).ready(function () {
        var currentSiteVar = window.location.pathname;
        $('li a').removeClass("active");
        $('a[href="' + currentSiteVar + '"').addClass('active');
        var route = currentSiteVar.split('/')[2];
        if (route == 'Index') {
            $('#btnCreate').show();
        }
        else {
            $('#btnCreate').hide();
        }
        // var createURL = "~/" + route + "/Create";
        $('#navCreate').attr("href", "Create", parent);
        $('ul li a').click(function () {
            $('li a').removeClass("active");
            $(this).addClass("active");
        });

        //$('a[href="~' + currentSiteVar + '"').addClass('active');
    });
    function DateConvertion(input) {
        var date = "";
        if (input != null) {
            var dateString = input.substr(6);
            var currentTime = new Date(parseInt(dateString));
            var month = currentTime.getMonth() + 1;
            var day = currentTime.getDate();
            var year = currentTime.getFullYear();
            date = day + "/" + month + "/" + year;
        }
        return date;
    }
</script>
