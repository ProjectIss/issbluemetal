@model issBlueMetal.Models.Sales

@{
    ViewBag.Title = "Delete";
}



<div class="breadcrumbs-inline pt-3 pb-1" id="breadcrumbs-wrapper">
    <div class="container">
        <div class="row">
            <div class="col s10 m6 l6 breadcrumbs-left">
                <h5 class="breadcrumbs-title mt-0 mb-0 display-inline hide-on-small-and-down"><span>Sales Delete</span></h5>

                <ol class="breadcrumbs mb-0">
                    <li class="breadcrumb-item">
                        <a href="~/Sales/Index">Index</a>
                    </li>
                    <li class="breadcrumb-item">
                        <a href="#">Delete</a>
                    </li>

                </ol>
            </div>

        </div>
    </div>
</div>
<div class="row">
    <div class="col s12">
        <div class="card card-tabs">
            <div class="card-content">

                
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Customer</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.customer.name)
                    </div>
                </div>
                
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Delivery Place</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.deliveryPlace)
                    </div>
                </div>
                
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Driver</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.driver)
                    </div>
                </div>
                
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Vehicle No</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.vehicle)
                    </div>
                </div>
                
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Bill No</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.billNo)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Date</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.Date)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Type</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.type)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Material Id</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.itemId)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Address</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.address)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Phone No</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.phoneNo)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">No Of Unit</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.noOfUnit)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Rate Per Unit</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.ratePerUnit)
                    </div>
                </div>
                
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Driver Salary</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.driverSalary)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Rent Amount</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.rentAmount)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Net Amount</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.netAmount)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">
                        GST Rate
                    </label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.advanceAmount)
                    </div>
                </div>
                
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Paid Amount</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.paidAmount)
                    </div>
                </div>
               
                <div class="form-group row">
                    <label class="col-lg-2 font-weight-bold">Balance Amount</label>:
                    <div class="col-lg-9">
                        @Html.DisplayFor(model => model.balanceAmount)
                    </div>
                </div>
              


                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color" style="text-align:center">
                        <input type="submit" value="Delete" class="btn btn-default" /> |
                        @Html.ActionLink("Back to List", "Index")
                    </div>
                }
            </div>
        </div>
    </div>
</div>
